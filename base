local HttpService = game:GetService("HttpService")
local databaseURL = "https://spatchfr-frjv-default-rtdb.firebaseio.com/scripts/monScript.json"

local response = HttpService:GetAsync(databaseURL)
local data = HttpService:JSONDecode(response)

if data.secretKey == "3RwRJQMUAF" then
    loadstring(data.code)()
else
    warn("Accès refusé : clé secrète invalide")
end 
